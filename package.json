{
  "name": "webpack-dll-spike-app",
  "version": "0.0.1",
  "description": "A simple spike using webpack and DLL plugin to power a core app and a plugin architecture based on static bundles (DLLs).",
  "keywords": [
    "webpack",
    "dll",
    "architecture",
    "plugin",
    "bundler"
  ],
  "author": "Tiago Costa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mistic/webpack-dll-spike-app.git"
  },
  "scripts": {
    "compile": "node ./bundler --mode=development --type=compiler",
    "compile:prod": "node ./bundler --mode=production --type=compiler",
    "clean": "rm -f -r -d build",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run compile && node ./bundler --mode=development --type=loader --target=webpack-dev-server",
    "start:prod": "npm run compile:prod && npm run load:prod && node express.js",
    "load": "node ./bundler --mode=development --type=loader",
    "load:prod": "node ./bundler --mode=production --type=loader"
  },
  "devDependencies": {
    "@types/node": "^10.5.0",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "image-webpack-loader": "^4.3.1",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "react-hot-loader": "^4.3.3",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "supports-color": "^5.4.0",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.9.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "highcharts": "^6.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
